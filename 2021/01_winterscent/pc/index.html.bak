@keyframes blurOn {

     0% {
	 	opacity:0;
	 	-webkit-transform:scale(1.2);
	 	transform:scale(1.2);
		filter: blur(10px);
		-webkit-filter: blur(10px);
     }
     100% {
	 	opacity:1;
	 	-webkit-transform:scale(1);
	 	transform:scale(1);
		filter: blur(0);
		-webkit-filter: blur(0);
     }
}


